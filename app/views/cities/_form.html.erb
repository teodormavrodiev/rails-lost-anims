<%= simple_form_for(@city) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
  </div>
  <div class="form-inputs">
    <%= f.input :photo, as: :attachinary %>
  </div>
  <div class="form-actions text-center margin-bottom">
    <%= f.button :submit %>
  </div>
<% end %>
